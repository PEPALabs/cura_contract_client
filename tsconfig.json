{
    "compilerOptions": {
      "target": "ES2020",
      "module": "CommonJS",
      "declaration": true,
      "outDir": "./dist",
      "rootDir": ".",
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"],
  }